      <section id="reviews">
        <h2>Parerile clientilor</h2>
        <div class="cont">
          <div>
            <h4>Tom Hanks</h4>
            <div class="star">
            <i class="fas fa-star"></i>
            <i class="fas fa-star"></i>
            <i class="fas fa-star"></i>
            <i class="fas fa-star"></i>
            <i class="fas fa-star"></i>
            </div>
